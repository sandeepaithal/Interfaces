package interfaces;

	/**
	 * Interface representing an employee in the payroll system.
	 * 
	 * @author C SANDEEP AITHAL
	 */
	public interface Employee {

	    /**
	     * Calculates the salary of the employee.
	     * 
	     * @return the salary of the employee
	     */
	    double calculateSalary();
	}

